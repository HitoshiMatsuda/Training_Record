<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.co.futureantiques.trainingrecord">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/training_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Custom_Design">
        <activity android:name=".Activity.MenuDetailActivity"></activity>
        <activity android:name=".Activity.AbsMainActivity" />
        <activity android:name=".Activity.MenuActivity" />
        <activity android:name=".Activity.EditActivity" />
        <activity android:name=".Activity.TrainingRegisterActivity" />
        <activity android:name=".Activity.WeightRegisterActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>